{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".json": "application/json",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".html": "text/html"
  },
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "Content-Type": "text/javascript"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "Content-Type": "text/css"
      }
    }
  ]
}
